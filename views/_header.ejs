<header>
    <h3>Page Header</h3>

    <ul>
        <% if(locals.user){ %>
            <li><a href="/users/profile"><%= user.name %></a></li>
            <li><a href="/users/log-out">Log out</a></li>
        <% } else{ %>
            <li><a href="/users/log-in">Log in</a></li>
            <li><a href="/users/sign-up">Sign up</a></li>
        <% } %>

    </ul>
</header>